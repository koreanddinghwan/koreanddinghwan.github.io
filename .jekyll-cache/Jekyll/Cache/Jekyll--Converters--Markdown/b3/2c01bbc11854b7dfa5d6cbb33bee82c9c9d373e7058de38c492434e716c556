        <span class="k">return</span> <span class="n">ch</span><span class="p">.</span><span class="nb">next</span> <span class="c1">#ë…¸ë“œë¦¬í„´
</span>
<span class="k">def</span> <span class="nf">last</span><span class="p">(</span><span class="bp">self</span><span class="p">):</span>
    <span class="n">ch</span> <span class="o">=</span> <span class="bp">self</span><span class="p">.</span><span class="n">head</span>
    <span class="k">return</span> <span class="n">ch</span><span class="p">.</span><span class="n">prev</span> <span class="c1">#ë…¸ë“œë¦¬í„´
