<pre><code class="language-js:toc.js">import React, { Component } from 'react';

class Toc extends Component {
  render() {
    var lst = []
    var data = this.props.data
    let i = 0
    while(i&lt;data.length) {
      lst.push(&lt;li id={data[i].id}&gt;{data[i].title}&lt;/li&gt;)
      i += 1
    }
    return (
      &lt;nav&gt;
        &lt;ul&gt;
          {lst}
        &lt;/ul&gt;
      &lt;/nav&gt;
    )
  }
}
</code></pre>

